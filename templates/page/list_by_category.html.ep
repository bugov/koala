% layout 'front';
% title $category->title . ' category';

<h1>Category "<%= $category->title %>"</h1>
<p><%= $category->legend %></p>

% for my $item (@$list) {
<div>
  <h2><%= $item->title %></h2>
  <p>
    <%= $item->legend %>
  </p>
  <p>
    <a href="<%= $item->url %>">more</a>
  </p>
</div>
% }